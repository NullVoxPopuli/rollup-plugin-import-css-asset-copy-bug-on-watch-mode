{
  "name": "repro",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "license": "GPL-3.0",
  "scripts": {
    "build": "rollup --config",
    "start": "rollup --config --watch --no-watch.clearScreen"
  },
  "devDependencies": {
    "@babel/core": "7.28.5",
    "@babel/plugin-proposal-decorators": "7.28.0",
    "@embroider/addon-dev": "8.1.0",
    "@embroider/macros": "1.19.2",
    "@rollup/plugin-babel": "6.0.4",
    "babel-plugin-ember-template-compilation": "2.4.1",
    "decorator-transforms": "^2.3.0",
    "ember-hbs-minifier": "1.3.0",
    "ember-scoped-css": "1.0.1",
    "ember-source": "6.6.0",
    "rollup": "4.52.4",
    "rollup-plugin-import-css": "^4.1.0"
  },
  "engines": {
    "node": ">= 24",
    "npm": "use pnpm",
    "yarn": "use pnpm"
  },
  "exports": {
    ".": {
      "types": "./declarations/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*.css": {
      "default": "./dist/*.css"
    },
    "./test-support": {
      "types": "./declarations/test-support/index.d.ts",
      "default": "./dist/test-support/index.js"
    },
    "./addon-main.cjs": "./addon-main.cjs"
  },
  "files": [
    "addon-main.cjs",
    "dist",
    "src",
    "declarations",
    "translations",
    "public"
  ]
}
